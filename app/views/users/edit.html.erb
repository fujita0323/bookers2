<h2>Editing Page</h2>
<%= form_with model: @user do |f| %>
<label for="inputName">Name</label>
<%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"Title" %>
<label for="inputImage">ProfileImage</label>
<%= f.file_field :profile_image, placeholder: "Opinion", accept: "image/*" %>
<%= f.submit "Update Book" %>
<% end %>